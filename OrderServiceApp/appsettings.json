{
  "ConnectionStrings": {
    "DefaultConnection": "Host={Host};Port={Port};Database={Database};Username={Username};Password={Password}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Debug"
    }
  },
  "AllowedHosts": "*",
